<div class="slider-viewport">
  <div id="banner{{ module }}" class="carousel slide" data-ride="carousel">
    <div class="carousel-inner">
      {% for banner in banners %}
        <div class="carousel-item {% if loop.first %}active{% endif %}">
          {% if banner.link %}
            <a href="{{ banner.link }}">
              <img src="{{ banner.image }}" alt="{{ banner.title }}" class="d-block w-25"/>
            </a>
          {% else %}
            <img src="{{ banner.image }}" alt="{{ banner.title }}" class="d-block w-25" />
          {% endif %}
        </div>
      {% endfor %}
    </div>
    <a class="carousel-control-prev" href="#banner{{ module }}" role="button" data-slide="prev">
      <i class="fas fa-chevron-left fa-3x text-secondary"></i>
    </a>
    <a class="carousel-control-next" href="#banner{{ module }}" role="button" data-slide="next">
      <i class="fas fa-chevron-right fa-3x text-secondary"></i>
    </a>
  </div>
  <ol class="carousel-indicators">
    {% for banner in banners %}
      <li data-target="#banner{{ module }}" data-slide-to=" {{loop.index }}" class="{% if loop.first %}active{% endif %}"></li>
    {% endfor %}
    </ol>
</div>